[application]

config/name="Super Seu Bento"
run/main_scene="res://scenes/levels/Intro.tscn"
config/use_custom_user_dir=true
boot_splash/bg_color=Color( 0.141176, 0.141176, 0.141176, 1 )
config/icon="res://icon.png"

[autoload]

Music="*res://singletons/Music.tscn"
Dialogs="*res://singletons/Dialogs.gd"
Inventory="*res://singletons/Inventory.gd"
Quest="*res://singletons/Quest.gd"
Globals="*res://singletons/Globals.gd"
DialogicSingleton="*res://addons/dialogic/Other/DialogicSingleton.gd"

[debug]

gdscript/warnings/return_value_discarded=false

[display]

window/size/width=320
window/size/height=180
window/size/borderless=true
window/size/fullscreen=true
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "AsepriteWizard", "dialogic" )

[gdnative]

singletons=[ "res://git_api.gdnlib" ]

